{
  "name": "lande",
  "repository": "github:fabiospampinato/lande",
  "description": "A tiny neural network for natural language detection.",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dataset:download": "wget https://downloads.tatoeba.org/exports/sentences.tar.bz2 -O resources/dataset.tar.bz2",
    "dataset:unpack": "tar -xf resources/dataset.tar.bz2 && mv sentences.csv resources/dataset.csv",
    "dataset:process": "bun src/process.ts",
    "dataset:stats": "bun src/stats.ts",
    "dataset:utils": "esbuild --bundle --format=esm --target=es6 src/utils.ts > standalone/utils.js",
    "update": "npm run dataset:download && npm run dataset:unpack && npm run dataset:process"
  },
  "keywords": [
    "tiny",
    "natural",
    "language",
    "detection",
    "neural",
    "network"
  ],
  "devDependencies": {
    "@types/langs": "^2.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.15",
    "esbuild": "^0.16.7",
    "langs": "^2.0.0",
    "lodash": "^4.17.21",
    "toygrad": "^2.3.0",
    "tsex": "^1.1.4",
    "typescript": "^4.9.4"
  }
}
